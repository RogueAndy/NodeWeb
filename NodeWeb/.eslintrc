{
  "extends": "eslint:recommended",
  "env":{
    "node":true,
    "es6":true,
    "jquery": true,
    "commonjs": true,
    "browser": true
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": "off",
    "no-console":"off",
    "no-unused-vars":"off",
    "no-unreachable":"off",
    "no-redeclare":"warn"
  }
}